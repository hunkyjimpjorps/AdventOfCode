#lang rhombus

import:
  rhombus/measure
  lib("advent-of-code/main.rkt") as aoc

export:
  get_day
  run

fun get_day(day):
  aoc.#{fetch-aoc-input}(aoc.#{find-session}(), 2025, day, ~cache: #true)

fun run(part_1, part_2):
  println(@str{Part 1: @(measure.time: ~gc; part_1())})
  println(@str{Part 2: @(measure.time: ~gc; part_2())})
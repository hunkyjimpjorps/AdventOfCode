#lang rhombus 

export:
  all_defined

class Posn(x, y):
  method all_neighbors():
    for List:
      each dx in [-1, 0, 1]
      each dy in [-1, 0, 1]
      skip_when dx == 0 && dy == 0
      Posn(x + dx, y + dy)
  method ortho_neighbors():
    for List:
      each:
        [dx, dy] in [[0, 1], [0, -1], [1, 0], [-1, 0]]
      skip_when dx == 0 && dy == 0
      Posn(x + dx, y + dy)
  method move(r, c):
      Posn(x + r, y + c)
  